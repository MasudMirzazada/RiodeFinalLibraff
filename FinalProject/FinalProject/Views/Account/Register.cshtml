@model RegisterVM
<section id="login">
    <div class="container">
        <div class="row login">
            <form id="loginForm" method="post" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly"></div>
                <input asp-for="FullName" placeholder="Full Name">
                <input asp-for="UserName" placeholder="User Name">
                <input asp-for="Email" placeholder="Email">
                <input asp-for="Password" placeholder="Password">
                <input asp-for="ConfirmPassword" placeholder="ConfirmPassword">
                <button type="submit" class="viewshopAbutton">Create</button>
                <div class="links mt-3">
                    <a href="">Return to Store</a>
                </div>
            </form>
        </div>
    </div>
</section>